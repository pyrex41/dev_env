{
  "name": "wander-api",
  "version": "1.0.0",
  "description": "Wander API - Node/TypeScript backend",
  "main": "dist/index.js",
  "scripts": {
    "dev": "node --inspect=0.0.0.0:9229 --require ts-node/register --watch src/index.ts",
    "dev:nodemon": "nodemon --exec 'node --inspect=0.0.0.0:9229 -r ts-node/register' src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "migrate": "node dist/migrations/run.js",
    "seed": "node dist/seeds/run.js",
    "lint": "eslint src --ext .ts"
  },
  "keywords": ["api", "typescript", "express"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "redis": "^4.6.12",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/cors": "^2.8.17",
    "@types/pg": "^8.11.10",
    "typescript": "^5.3.3",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0"
  }
}
